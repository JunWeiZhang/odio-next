{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@containers": "./src/containers",
          "@utils": "./src/utils",
          "@helpers": "./src/helpers",
          "@config": "./src/config",
          "@redux": "./src/redux",
          "@style": "./src/style",
          "@constants": "./src/constants",
          "@api": "./src/api"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
